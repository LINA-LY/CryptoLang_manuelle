const char* code_source = 
    "@protocol ProgrammeCorrect\n"
    "\n"
    "@keyspace {\n"
    "    byte :: compteur = 5;\n"
    "}\n"
    "\n"
    "@main {\n"
    "    plain :: msg = \"Secret\";\n"
    "    \n"
    "    @loop [ compteur > 0 ] {\n"
    "        compteur = compteur - 1;\n"
    "    }\n"
    "    \n"
    "    hash :: h = msg #>;\n"
    "    -> h;\n"
    "}\n"
    "\n"
    "@endprotocol";